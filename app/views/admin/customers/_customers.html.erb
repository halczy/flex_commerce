<table class="table table-hover">
  <thead>
    <tr>
      <th>Member ID</th>
      <th>Name</th>
      <th>Cell Number</th>
      <th>Email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% customers.each do |customer| %>
    <tr>
      <td><%= link_to customer.member_id %></td>
      <td><%= link_to customer.name, admin_customer_path(customer) %></td>
      <td><%= customer.cell_number %></td>
      <td><%= customer.email %></td>
      <td>
        <%= link_to 'Detail', admin_customer_path(customer),
                              class: 'btn btn-info btn-sm' %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
