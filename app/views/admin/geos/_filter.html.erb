<%= form_with(url: admin_geos_path, local: true, method: :get) do |form| %>
  <%= form.select :geo_filter, options_for_select([
                  ['No Filter', ''],
                  ['Country/Region', 'country_region'],
                  ['Province/State', 'province_state'],
                  ['City', 'city'],
                  ['District', 'district'],
                  ['Community', 'community']]),
                  { prompt: "Filter Geo Data" },
                  class: 'form-control', onchange: 'this.form.submit();' %>
<% end %>
