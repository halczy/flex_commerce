<%= provide :title, 'Geographical Data' %>

<div class="d-flex justify-content-start mb-3">
  <div class="p-2">
    <%= link_to admin_shipping_methods_path, class: 'btn btn-secondary' do %>
      <i class="fa fa-arrow-left" aria-hidden="true"></i> Return
    <% end %>
  </div>
  <div class="p-2">
    <%= render 'filter' %>
  </div>
  <div class="ml-auto p-2">
    <%= render 'search_form' %>
  </div>
</div>

<div class="row">
  <%= render 'shared/paginate', objects: @geos if @geos.present? %>
  <div class="col-lg-12">
    <p>Geo Count: <%= @geos.total_count %></p>
    <%= render 'geos', geos: @geos %>
  </div>
  <%= render 'shared/paginate', objects: @geos if @geos.present? %>
</div>
