<table class="table table-hover">
  <thead>
    <tr>
      <th>Geo Code</th>
      <th>Name</th>
      <th>Parent Code</th>
      <th>Parent Name</th>
    </tr>
  </thead>
  <tbody>
    <% geos.each do |geo| %>
    <tr>
      <td><%= geo.id %></td>
      <td><%= geo.name %></td>
      <td><%= geo.parent.try(:id) %></td>
      <td><%= geo.parent.try(:name) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
