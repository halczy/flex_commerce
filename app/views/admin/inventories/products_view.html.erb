<%= provide :title, 'Inventory' %>

<div class="row mb-5">
  <div class="col">
    <%= link_to '#', class: 'btn btn-success' do %>
      <i class="fa fa-bars" aria-hidden="true"></i> Inventory Master
    <% end %>
    <%= link_to '#', class: 'btn btn-light' do %>
      <i class="fa fa-list-alt" aria-hidden="true"></i> Show In Stock Products
    <% end %>
    <%= link_to '#', class: 'btn btn-light' do %>
      <i class="fa fa-list-alt" aria-hidden="true"></i> Show Out of Stock Products
    <% end %>
  </div>
</div>

<div class="row">
  <%= render 'shared/paginate', objects: @products if @products.present? %>
  <div class="col-lg-12">
    <%= render 'products_inventories', products: @products %>
  </div>
  <%= render 'shared/paginate', objects: @products if @products.present? %>
</div>
